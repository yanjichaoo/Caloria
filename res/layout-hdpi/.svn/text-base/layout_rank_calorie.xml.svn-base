<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<include layout="@layout/layout_title_bar" />
	<LinearLayout android:id="@+id/rankTopLayout"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:background="@drawable/layout_black_bg" android:layout_margin="20dp">
		<TextView style="@style/rank_chart_top_text"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:id="@+id/rank_text_title" android:layout_margin="10dp"
			android:gravity="center"></TextView>
	</LinearLayout>
	<LinearLayout android:layout_width="match_parent"
		android:layout_height="wrap_content" android:background="@drawable/layout_black_bg"
		android:layout_marginLeft="20dp" android:layout_marginRight="20dp"
		android:layout_marginBottom="20dp" android:orientation="vertical">
		<TextView android:text="@string/rank_county_label"
			android:id="@+id/textView1" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:layout_marginTop="10dp"
			android:layout_marginBottom="10dp" android:gravity="center"></TextView>
		<LinearLayout android:id="@+id/rankchartLayout"
			android:layout_width="match_parent" android:layout_height="wrap_content">
		</LinearLayout>
	</LinearLayout>
</LinearLayout>